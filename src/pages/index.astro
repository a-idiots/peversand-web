---
import Layout from '../layouts/Layout.astro';
---

<Layout title="Home" noScroll>
  <section class="hero">
    <img src="/hero.jpg" alt="Peanut Butter Sandwich" class="hero-image" />
  </section>
</Layout>

<style>
  .hero {
    position: relative;
    width: 100%;
    height: 100%;
    overflow: hidden;
  }

  .hero-image {
    width: 100%;
    height: 100%;
    object-fit: cover;
  }


</style>
